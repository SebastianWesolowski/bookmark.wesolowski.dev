{
  "name": "react-learn-bookmark",
  "description": "Simple React app - React learn -Bookmark ",
  "author": "Sebastian Weso≈Çowski <sebastian@wesolowski.dev>",
  "version": "0.1.0",
  "scripts": {
    "start": "yarn develop",
    "develop": "cross-env GATSBY_GRAPHQL_IDE=playground gatsby develop",
    "serve": "gatsby serve",
    "ssr": "yarn build && yarn serve",
    "build:dir": "mkdirp ./public/icons/",
    "build:favicons": "node scripts/favicons",
    "build": "yarn build:dir && yarn build:favicons && gatsby build",
    "format": "prettier --config ./.prettierrc --write \"**/*.{js,jsx,json,md}\"",
    "analyze": "ANALYZE_BUNDLE_SIZE=true gatsby build",
    "test": "ava **/*.test.js --verbose"
  },
  "greenkeeper": {
    "ignore": [
      "core-js"
    ]
  },
  "private": true,
  "keywords": [
    "gatsby",
    "React Learn",
    "Bookmark"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SebastianWesolowski/react-learn-bookmark"
  },
  "bugs": {
    "url": "https://github.com/SebastianWesolowski/react-learn-bookmark/issues"
  },
  "dependencies": {
    "@danbruegge/gatsby-plugin-stylelint": "^4.0.0",
    "cross-env": "^7.0.0",
    "gatsby": "^2.15.34",
    "gatsby-image": "^2.2.25",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.1.11",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-sharp": "^2.2.29",
    "gatsby-plugin-sitemap": "^2.2.17",
    "gatsby-plugin-styled-components": "^3.1.9",
    "gatsby-plugin-web-font-loader": "^1.0.4",
    "gatsby-plugin-webpack-bundle-analyzer": "^1.0.5",
    "gatsby-plugin-webpack-size": "^1.0.0",
    "gatsby-source-filesystem": "^2.1.31",
    "gatsby-transformer-json": "^2.2.13",
    "gatsby-transformer-sharp": "^2.2.21",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "styled-components": "^5.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.0.3",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-loader": "^3.0.2",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "favicons": "^5.5.0",
    "husky": "^4.2.2",
    "lint-staged": "^10.0.7",
    "mkdirp": "^1.0.3",
    "prettier": "^1.18.2",
    "stylelint": "^13.1.0",
    "stylelint-config-recommended": "^3.0.0",
    "stylelint-config-styled-components": "^0.1.1",
    "stylelint-processor-styled-components": "^1.8.0",
    "stylelint-webpack-plugin": "^1.0.2"
  }
}
